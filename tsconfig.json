{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "declaration": false,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist", ".wwebjs*"]
}
